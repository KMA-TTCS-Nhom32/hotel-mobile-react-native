# Environment Variables
# Copy this file to .env.local and update the values

# API Configuration
EXPO_PUBLIC_API_URL=https://api.ahomevilla.com
EXPO_PUBLIC_API_VERSION=v1

# App Configuration  
EXPO_PUBLIC_APP_NAME=AHomeVilla
EXPO_PUBLIC_COMPANY_NAME=AHomeVilla Hotels

# Development settings
EXPO_PUBLIC_ENABLE_LOGS=true
EXPO_PUBLIC_ENABLE_FLIPPER=true

